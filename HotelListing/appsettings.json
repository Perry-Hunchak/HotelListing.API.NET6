{
  "ConnectionStrings": {
    "HotelListingDBConnectString": "Server=(localdb)\\mssqllocaldb;Database=HotelListingApiDb;Trusted_Connection=True;MultipleActiveResultSets=True"
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Overrdie": {
        "Microsoft": "Warning",
        "Microsfot.Hosting.Lifetime": "Information"
      }
    },
    "WriteTo": [
      {
        "Name": "File",
        "Args": {
          "path": "./logs/log-.txt",
          "rollingInterval": "Day"
        }
      },
      {
        "Name": "Seq",
        "Args": { "serverUrl": "http://localhost:5341" }
      }
    ]
  },
  "AllowedHosts": "*"
}
